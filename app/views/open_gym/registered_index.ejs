<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Open Gym Registration</title>
    <link rel="stylesheet" href="/css/open_gym.css">
    <link rel="stylesheet" href="/css/style.css">

    <script src="/js/jquery-3.1.1.min.js" charset="utf-8"></script>
    <script src="/js/open_gym.js" charset="utf-8"></script>

  </head>
  <body>
    <% include ../layouts/header %>

    <div class="main-container">

      <table class="users-table">
        <thead>
          <td>Child Name</td>
          <td>Parent Name</td>
          <td>Emergency Contact Name</td>
          <td>Emergency Contact Number</td>
          <td>Address</td>
          <td>Medical Notes</td>
        </thead>
        <tbody>
          <% for(var i=0;i < children.length;i++){ %>
            <tr>
              <td><%= children[i].full_name() %> </td>
              <td><%= parent_hash[children[i].legal_guardian_id[0]]%></td>
              <td><%= children[i].emergency_contact_name %></td>
              <td><%= children[i].emergency_contact_phone%></td>
              <td><%= children[i].address %></td>
              <td><%= children[i].medical_notes %></td>
            </tr>

          <% } %>
        </tbody>
      </table>


    </div>


  </body>
</html>
