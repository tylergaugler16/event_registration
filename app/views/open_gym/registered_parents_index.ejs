<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Open Gym Registration</title>
    <link rel="stylesheet" href="/stylesheets/simple-grid.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
    <link rel="stylesheet" href="/stylesheets/open_gym.css">


    <script src="/js/jquery-3.1.1.min.js" charset="utf-8"></script>
    <script src="/js/open_gym.js" charset="utf-8"></script>

  </head>
  <body>
    <% include ../layouts/header %>
    <% include ../layouts/message %>
    <div class="main-container">

      <table class="users-table">
        <thead>
          <td>Parent Name</td>
          <td>Email</td>
          <td>Number of Children Registered</td>
        </thead>
        <tbody>
          <% for(var i=0;i < parents.length;i++){ %>
            <tr>
              <td><a href="/users/<%= parents[i]._id %>"><%= parents[i].full_name() %></a></td>
              <td><%= parents[i].email %></td>
              <td>
                  <%= parents[i].children.length %>
              </td>
            </tr>

          <% } %>

        </tbody>
      </table>



    </div>


  </body>
</html>
